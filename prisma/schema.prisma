generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://root:root@localhost:5432/rbac?schema=public"
}

model role {
  id         Int      @id(map: "PK_b36bcfe02fc8de3c57a8b2391c2") @default(autoincrement())
  name       String   @unique(map: "UQ_ae4578dcaed5adff96595e61660") @db.VarChar
  created_at DateTime @default(dbgenerated("('now'::text)::timestamp(6) with time zone")) @db.Timestamp(6)
  updated_at DateTime @default(dbgenerated("('now'::text)::timestamp(6) with time zone")) @db.Timestamp(6)
  user       user[]
}

model user {
  id         Int      @id(map: "PK_cace4a159ff9f2512dd42373760") @default(autoincrement())
  username   String   @unique(map: "UQ_78a916df40e02a9deb1c4b75edb") @db.VarChar
  password   String   @db.VarChar
  created_at DateTime @default(dbgenerated("('now'::text)::timestamp(6) with time zone")) @db.Timestamp(6)
  updated_at DateTime @default(dbgenerated("('now'::text)::timestamp(6) with time zone")) @db.Timestamp(6)
  role_id    Int?
  role       role?    @relation(fields: [role_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_fb2e442d14add3cefbdf33c4561")
}
