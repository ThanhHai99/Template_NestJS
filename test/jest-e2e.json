{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@libs/redis/(.*)": "<rootDir>/../libs/redis/src/$1",
    "@libs/redis": "<rootDir>/../libs/redis/src"
  }
}
